<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   
  <bean id="mpsResourceType" class="edu.yu.einstein.wasp.load.ResourceTypeLoader">
    <property name="name" value="Massively Parallel DNA Sequencer" />
    <property name="area" value="mps" />
  </bean>
  
  <bean id="ampliconResourceType" class="edu.yu.einstein.wasp.load.ResourceTypeLoader">
    <property name="name" value="DNA Amplicon" />
    <property name="area" value="amplicon" />
  </bean>
  
  <bean id="alignerResourceType" class="edu.yu.einstein.wasp.load.ResourceTypeLoader">
    <property name="name" value="Aligner" />
    <property name="area" value="aligner" />
  </bean>
  
  <bean id="peakcallerResourceType" class="edu.yu.einstein.wasp.load.ResourceTypeLoader">
    <property name="name" value="Peak Caller" />
    <property name="area" value="peakcaller" />
  </bean>
  
  <bean id="sequenceRunProcessorResourceType" class="edu.yu.einstein.wasp.load.ResourceTypeLoader">
    <property name="name" value="Sequence Run Processor" />
    <property name="area" value="sequenceRunProcessor" />
  </bean>
 
</beans>

