<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   
  <bean id="biomaterialSampleTypeCategory" class="edu.yu.einstein.wasp.load.SampleTypeCategoryLoaderAndFactory" >
    <property name="name" value="Biomaterial" />
    <property name="area" value="biomaterial" />
  </bean>
  
  <bean id="hardwareSampleTypeCategory" class="edu.yu.einstein.wasp.load.SampleTypeCategoryLoaderAndFactory" >
    <property name="name" value="Hardware" />
    <property name="area" value="hardware" />
  </bean>
  
  
 
</beans>

