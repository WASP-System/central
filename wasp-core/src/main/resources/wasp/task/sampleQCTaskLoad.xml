<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="sampleQCTaskMapping" class="edu.yu.einstein.wasp.load.TaskMappingLoader">
		<property name="iName" value="sampleQCTask" />
		<property name="name" value="Library QC" />
		<property name="taskMapping">
			<list>
				<bean class="edu.yu.einstein.wasp.model.TaskMapping">
	          		<property name="stepName" value="wasp.library.step.libraryQC" />
					<property name="status" value="STARTED" />
					<property name="listMap" value="/sample/pendingqc/list.do" />
					<property name="permission" value="hasRole('su') or hasRole('fm') or hasRole('ft')" />
				</bean>
			</list>
		</property>
	</bean>
</beans>
