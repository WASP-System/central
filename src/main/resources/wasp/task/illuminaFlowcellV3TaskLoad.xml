<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
    default-autowire="byName">

  <bean id="placeAmpliconTask" class="edu.yu.einstein.wasp.load.TaskLoadService">
    <property name="iName" value="placeAmplicon" />
    <property name="name" value="Place Amplicon" />
  </bean>

  <bean id="returnAmpliconTask" class="edu.yu.einstein.wasp.load.TaskLoadService">
    <property name="iName" value="returnAmplicon" />
    <property name="name" value="Return from Amplicon" />
  </bean>
</beans>
