<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   
   <!-- *********** control library name MUST BE UNIQUE **************** -->
   <!-- *********** currently, only control libraries without adaptors without organism are possible **************** -->
   
  <bean id="controlLibraryPhiX" class="edu.yu.einstein.wasp.load.ControlLibraryLoaderAndFactory">
    <property name="name" value="PhiX (no index)" />
    <property name="sampleType" ref="librarySampleType" />
    <property name="sampleSubtype" ref="controlLibrarySampleSubtypeResource" />    
  </bean>
  
</beans>

