<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="genericDnaSeqDnaSampleSubtypeResource" class="edu.yu.einstein.wasp.load.SampleSubtypeLoader">
		<property name="name" value="Generic DNA Seq DNA" />
		<property name="area" value="genericDnaDnaSample" />
		<property name="domain" value="#{genericDnaSeqPluginDomain}" />
		<property name="sampleType" value="#{dnaSampleType.area}" />
		<property name="uiFieldGroupsFromWrapper">
			<list>
				<ref bean="genericBiomoleculeUiFields" />
				<ref bean="genericDNAUiFields" />
			</list>
		</property>
		<property name="applicableRoles" value="ft,lu" />

	</bean>

	<bean id="genericDnaSeqLibrarySampleSubtypeResource" class="edu.yu.einstein.wasp.load.SampleSubtypeLoader">
		<constructor-arg ref="genericDnaSeqDnaSampleSubtypeResource" />
		<property name="name" value="Generic DNA Seq Library" />
		<property name="area" value="genericDnaLibrarySample" />
		<property name="domain" value="#{genericDnaSeqPluginDomain}" />
		<property name="sampleType" value="#{librarySampleType.area}" />
		<property name="applicableRoles" value="lu" />
		<property name="uiFieldGroupsFromWrapper">
			<list>
				<ref bean="genericBiomoleculeUiFields" />
				<ref bean="genericLibraryUiFields" />
			</list>
		</property>
	</bean>

	<bean id="genericDnaSeqFacilityLibrarySampleSubtypeResource" class="edu.yu.einstein.wasp.load.SampleSubtypeLoader">
		<constructor-arg ref="genericDnaSeqDnaSampleSubtypeResource" />
		<property name="name" value="Generic DNA Seq Facility Library" />
		<property name="area" value="genericDnaFacilityLibrarySample" />
		<property name="domain" value="#{genericDnaSeqPluginDomain}" />
		<property name="sampleType" value="#{librarySampleType.area}" />
		<property name="applicableRoles" value="ft" />
		<property name="uiFieldGroupsFromWrapper">
			<list>
				<ref bean="genericLibraryUiFields" />
			</list>
		</property>
	</bean>

</beans>
