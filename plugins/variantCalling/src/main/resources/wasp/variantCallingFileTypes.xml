<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
   <bean id="bamDedupRealnRecalFileType" class="edu.yu.einstein.wasp.load.FileTypeLoaderAndFactory" >
    <property name="name" value="BAM" />
    <property name="area" value="bamDedupRealnRecal" />
    <property name="description" value="Bam files after pre-processing via the GATK best practices pipeline, with removal of PCR duplicates, realignment around indels and quality score recalibration" />
	<property name="extension" value="bam" />
  </bean>


</beans>

