<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   
  <bean id="waspIlluminaPlatformQcMetricsFileType" class="edu.yu.einstein.wasp.load.FileTypeLoaderAndFactory" >
    <property name="name" value="Illumina HiSeq QC Metrics" />
    <property name="area" value="waspIlluminaPlatformQcMetrics" />
    <property name="description" value="QC files generated by CASAVA for assessing HiSeq flowcell / sequencing quality" />
  </bean>

</beans>