<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.1.xsd"
    default-autowire="byName">



  <bean id="helptagLibraryArea" class="java.lang.String">
  	<constructor-arg><value>helptagLibrary</value></constructor-arg>
  </bean>
    
  <bean class="edu.yu.einstein.wasp.model.UiField" id="helptagLibraryUiField">
    <property name="locale" value="en_US" />
    <property name="area" value="#{helptagLibraryArea}" />
  </bean>
   
  <bean id="helptagLibraryAdaptorsetLoad" class="edu.yu.einstein.wasp.load.AdaptorsetLoadService" >
  	<property name="area" value="#{helptagLibraryArea}" />
  	<property name="name" value="HELP-tag Library" />
  	<property name="sampleTypeIName" value="library" />
  	<property name="compatibleResourcesByIName">
  		<list>
  			<bean class="java.lang.String" depends-on="illuminaHiSeq2000">
  				<constructor-arg type="String" value="illuminaHiSeq2000" />
  			</bean>
  		</list>
  	</property>
  	<property name="adaptorList">
  		<list>
  			<bean class="edu.yu.einstein.wasp.model.Adaptor">
  				<property name="IName" value="illuminaHelptagLibrary1" />
  				<property name="name" value="helptag Adaptor" />
  				<property name="sequence" value="CGCTGCTG" />
  				<property name="barcodesequence" value="CGCTGCTG" />
  				<property name="barcodenumber" value="1" />
  			</bean>
   		</list>
  	</property>
  </bean> 

</beans>
