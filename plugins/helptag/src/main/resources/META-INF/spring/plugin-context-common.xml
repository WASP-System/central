<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans" default-autowire="byName"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="helpTagPluginDomain" class="java.lang.String">
		<constructor-arg>
			<value>edu.yu.einstein.wasp.helptag</value>
		</constructor-arg>
	</bean>

	<bean id="helpTagPluginArea" class="java.lang.String">
		<constructor-arg>
			<value>helpTagPlugin</value>
		</constructor-arg>
	</bean>

</beans>
