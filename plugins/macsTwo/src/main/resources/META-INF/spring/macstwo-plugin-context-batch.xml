<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans"
	default-autowire="byName"
	xmlns:batch="http://www.springframework.org/schema/batch"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/batch http://www.springframework.org/schema/batch/spring-batch.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="edu.yu.einstein.wasp.macstwo.batch.service.impl.MacstwoBatchServiceImpl" />
	
	<!-- common definitions -->
	<import resource="macstwo-plugin-context-common.xml" />
	
	<!-- statics for tasks associated with messages  -->
	<bean id="macstwoBatchJobTask" class="edu.yu.einstein.wasp.macstwo.integration.messages.MacstwoBatchJobTask"/>
	
</beans>
